loadstring([[

-- SETTINGS
local ballName = "Football"
local magnetRange = 35
local walkSpeed = 16
local pullVectorEnabled = false
local pullVectorStrength = 0.2
local magnetEnabled = true
local autoTuckEnabled = false

-- SERVICES
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local lp = Players.LocalPlayer
local playerGui = lp:WaitForChild("PlayerGui")

-- COLOR THEMES
local colorThemes = {
    {name="Black", primary=Color3.fromRGB(0,0,0), secondary=Color3.fromRGB(30,30,30)},
    {name="Blue", primary=Color3.fromRGB(0,162,255), secondary=Color3.fromRGB(30,30,30)},
    {name="Red", primary=Color3.fromRGB(255,50,50), secondary=Color3.fromRGB(30,30,30)},
    {name="Green", primary=Color3.fromRGB(50,255,50), secondary=Color3.fromRGB(30,30,30)},
    {name="Yellow", primary=Color3.fromRGB(255,255,50), secondary=Color3.fromRGB(30,30,30)}
}
local currentTheme = 2
local currentMenuColor = 1

-- GUI
local gui = Instance.new("ScreenGui")
gui.Name = "TacariGUI"
gui.Parent = playerGui

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0,450,0,350)
frame.Position = UDim2.new(0.5,-225,0.5,-175)
frame.BackgroundColor3 = colorThemes[currentMenuColor].secondary
frame.BorderSizePixel = 0
frame.Parent = gui

local uicorner = Instance.new("UICorner")
uicorner.CornerRadius = UDim.new(0,12)
uicorner.Parent = frame

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1,0,0,30)
title.Position = UDim2.new(0,0,0,0)
title.BackgroundTransparency = 1
title.Text = "Tacari | Elijah"
title.Font = Enum.Font.SourceSansBold
title.TextSize = 20
title.TextColor3 = Color3.fromRGB(255,255,255)
title.TextScaled = true
title.Parent = frame

-- TAB FRAMES
local tabFrame = Instance.new("Frame")
tabFrame.Size = UDim2.new(1,0,0,30)
tabFrame.Position = UDim2.new(0,0,0,40)
tabFrame.BackgroundTransparency = 1
tabFrame.Parent = frame

local mainTab = Instance.new("TextButton")
mainTab.Size = UDim2.new(0,80,1,0)
mainTab.Position = UDim2.new(0,0,0,0)
mainTab.BackgroundColor3 = colorThemes[currentTheme].primary
mainTab.Text = "Main"
mainTab.Font = Enum.Font.SourceSansBold
mainTab.TextColor3 = Color3.new(1,1,1)
mainTab.Parent = tabFrame

local miscTab = Instance.new("TextButton")
miscTab.Size = UDim2.new(0,80,1,0)
miscTab.Position = UDim2.new(0,90,0,0)
miscTab.BackgroundColor3 = Color3.fromRGB(60,60,60)
miscTab.Text = "Misc"
miscTab.Font = Enum.Font.SourceSansBold
miscTab.TextColor3 = Color3.new(1,1,1)
miscTab.Parent = tabFrame

local mainContent = Instance.new("Frame")
mainContent.Size = UDim2.new(1,-20,1,-80)
mainContent.Position = UDim2.new(0,10,0,80)
mainContent.BackgroundTransparency = 1
mainContent.Visible = true
mainContent.Parent = frame

local miscContent = Instance.new("Frame")
miscContent.Size = UDim2.new(1,-20,1,-80)
miscContent.Position = UDim2.new(0,10,0,80)
miscContent.BackgroundTransparency = 1
miscContent.Visible = false
miscContent.Parent = frame

-- FUNCTION TO CREATE SLIDERS
local function createSlider(parent,y,text,value,max,callback)
    local lbl = Instance.new("TextLabel")
    lbl.Size = UDim2.new(1,0,0,25)
    lbl.Position = UDim2.new(0,0,0,y)
    lbl.BackgroundTransparency = 1
    lbl.Text = text.." "..value
    lbl.TextColor3 = Color3.fromRGB(255,255,255)
    lbl.Font = Enum.Font.SourceSansBold
    lbl.TextSize = 14
    lbl.TextXAlignment = Enum.TextXAlignment.Left
    lbl.Parent = parent

    local back = Instance.new("Frame")
    back.Size = UDim2.new(1,0,0,8)
    back.Position = UDim2.new(0,0,0,y+30)
    back.BackgroundColor3 = Color3.fromRGB(30,30,30)
    back.Parent = parent
    local backCorner = Instance.new("UICorner")
    backCorner.CornerRadius = UDim.new(0,4)
    backCorner.Parent = back

    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(value/max,0,1,0)
    btn.Position = UDim2.new(0,0,0,0)
    btn.BackgroundColor3 = colorThemes[currentTheme].primary
    btn.Text = ""
    btn.BorderSizePixel = 0
    btn.Parent = back
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0,8)
    btnCorner.Parent = btn

    local dragging = false
    btn.MouseButton1Down:Connect(function() dragging = true end)
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType==Enum.UserInputType.MouseButton1 then dragging=false end
    end)

    RunService.RenderStepped:Connect(function()
        if dragging then
            local mouseX = UserInputService:GetMouseLocation().X
            local relX = math.clamp(mouseX - back.AbsolutePosition.X,0,back.AbsoluteSize.X)
            btn.Position = UDim2.new(0,relX,0,0)
            value = math.floor((relX/back.AbsoluteSize.X)*max)
            lbl.Text = text.." "..value
            callback(value)
        end
    end)
end

-- MAIN TAB SLIDERS AND BUTTONS
createSlider(mainContent,0,"Magnet Range",magnetRange,100,function(v) magnetRange=v end)
createSlider(mainContent,60,"WalkSpeed",walkSpeed,100,function(v)
    walkSpeed=v
    if lp.Character and lp.Character:FindFirstChild("Humanoid") then
        lp.Character.Humanoid.WalkSpeed=v
    end
end)
createSlider(mainContent,120,"Pull Vector Strength",pullVectorStrength*100,100,function(v) pullVectorStrength=v/100 end)

-- TOGGLE BUTTON FUNCTION
local function createToggle(parent,y,text,default,callback)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1,0,0,30)
    btn.Position = UDim2.new(0,0,0,y)
    btn.BackgroundColor3 = default and colorThemes[currentTheme].primary or Color3.fromRGB(80,80,80)
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.Font = Enum.Font.SourceSansBold
    btn.TextSize = 14
    btn.Text = text..": "..(default and "ON" or "OFF")
    btn.Parent = parent
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0,8)
    corner.Parent = btn

    local state = default
    btn.MouseButton1Click:Connect(function()
        state = not state
        btn.BackgroundColor3 = state and colorThemes[currentTheme].primary or Color3.fromRGB(80,80,80)
        btn.Text = text..": "..(state and "ON" or "OFF")
        callback(state)
    end)
end

-- AutoTuck toggle
createToggle(mainContent,180,"Auto Tuck",autoTuckEnabled,function(state) autoTuckEnabled=state end)
-- Pull Vector toggle
createToggle(mainContent,220,"Pull Vector",pullVectorEnabled,function(state) pullVectorEnabled=state end)
-- Magnet toggle
createToggle(mainContent,260,"Magnet",magnetEnabled,function(state) magnetEnabled=state end)
-- Rejoin button
local rejoinBtn = Instance.new("TextButton")
rejoinBtn.Size = UDim2.new(1,0,0,30)
rejoinBtn.Position = UDim2.new(0,0,0,300)
rejoinBtn.BackgroundColor3=colorThemes[currentTheme].primary
rejoinBtn.TextColor3=Color3.fromRGB(255,255,255)
rejoinBtn.Font=Enum.Font.SourceSansBold
rejoinBtn.TextSize=14
rejoinBtn.Text="Rejoin"
rejoinBtn.Parent=mainContent
local corner = Instance.new("UICorner")
corner.CornerRadius=UDim.new(0,8)
corner.Parent=rejoinBtn
rejoinBtn.MouseButton1Click:Connect(function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, lp)
end)

-- TAB SWITCH
mainTab.MouseButton1Click:Connect(function()
    mainContent.Visible=true
    miscContent.Visible=false
    mainTab.BackgroundColor3=colorThemes[currentTheme].primary
    miscTab.BackgroundColor3=Color3.fromRGB(60,60,60)
end)
miscTab.MouseButton1Click:Connect(function()
    mainContent.Visible=false
    miscContent.Visible=true
    mainTab.BackgroundColor3=Color3.fromRGB(60,60,60)
    miscTab.BackgroundColor3=colorThemes[currentTheme].primary
end)

-- DRAGGING
local dragStart
local startPos
title.InputBegan:Connect(function(input)
    if input.UserInputType==Enum.UserInputType.MouseButton1 then
        dragStart=input.Position
        startPos=frame.Position
    end
end)
title.InputChanged:Connect(function(input)
    if input.UserInputType==Enum.UserInputType.MouseMovement and dragStart then
        local delta=input.Position-dragStart
        frame.Position=UDim2.new(startPos.X.Scale,startPos.X.Offset+delta.X,startPos.Y.Scale,startPos.Y.Offset+delta.Y)
    end
end)
title.InputEnded:Connect(function(input) if input.UserInputType==Enum.UserInputType.MouseButton1 then dragStart=nil end end)

-- GUI TOGGLE
local guiVisible=false
UserInputService.InputBegan:Connect(function(input,processed)
    if processed then return end
    if input.KeyCode==Enum.KeyCode.RightControl then
        guiVisible=not guiVisible
        frame.Visible=guiVisible
    end
end)

-- MAGNET & PULL VECTOR
UserInputService.InputBegan:Connect(function(input,processed)
    if processed then return end
    if input.UserInputType==Enum.UserInputType.MouseButton1 then
        local char = lp.Character
        if not char then return end
        local hand = char:FindFirstChild("RightHand") or char:FindFirstChild("Right Arm") or char:FindFirstChild("HumanoidRootPart")
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if not hand or not hrp then return end
        for _,obj in ipairs(workspace:GetDescendants()) do
            if obj:IsA("BasePart") and obj.Name:lower():match(ballName:lower()) then
                local dist = (obj.Position-hrp.Position).Magnitude
                if magnetEnabled and dist<=magnetRange then
                    obj.CFrame=CFrame.new(hand.Position)
                elseif pullVectorEnabled and dist<=magnetRange then
                    game:GetService("TweenService"):Create(obj,TweenInfo.new(pullVectorStrength,Enum.EasingStyle.Linear),{CFrame=CFrame.new(hand.Position)}):Play()
                end
            end
        end
    end
end)

-- FORCEFIELD
local function createForcefield()
    local char = lp.Character or lp.CharacterAdded:Wait()
    local root = char:WaitForChild("HumanoidRootPart")
    local ff = Instance.new("Part")
    ff.Name="BlueForcefield"
    ff.Shape=Enum.PartType.Ball
    ff.Anchored=true
    ff.CanCollide=false
    ff.Transparency=0.6
    ff.Material=Enum.Material.ForceField
    ff.Color=colorThemes[currentTheme].primary
    ff.CastShadow=false
    ff.Size=Vector3.new(magnetRange*2,magnetRange*2,magnetRange*2)
    ff.Parent=workspace
    RunService.Heartbeat:Connect(function()
        if root and root.Parent then
            ff.Position=root.Position
            ff.Size=Vector3.new(magnetRange*2,magnetRange*2,magnetRange*2)
        else ff:Destroy() end
    end)
end
lp.CharacterAdded:Connect(createForcefield)
if lp.Character then createForcefield() end

-- AUTO TUCK
local function setupTool(tool)
    if tool:IsA("Tool") and tool.Name=="Football" and autoTuckEnabled then
        tool.Equipped:Connect(function()
            task.wait(0.1)
            VirtualInputManager:SendKeyEvent(true,Enum.KeyCode.E,false,game)
            task.wait(0.05)
            VirtualInputManager:SendKeyEvent(false,Enum.KeyCode.E,false,game)
        end)
    end
end
lp.Backpack.ChildAdded:Connect(setupTool)
lp.CharacterAdded:Connect(function(char) char.ChildAdded:Connect(setupTool) end)
if lp.Character then lp.Character.ChildAdded:Connect(setupTool) end

]])()
