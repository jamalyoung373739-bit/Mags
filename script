-- Kyng's UF Premium Script
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/jamalyoung373739-bit/Mags/refs/heads/main/script"))()
local window = library:init("Kyng's UF Premium", true, Enum.KeyCode.RightShift, true)

-- [DONT TOUCH / STARTUP]
window:Divider("ROBLOX FOOTBALL")
local sectionA = window:Section("RCFA SECTION")
local sectionB = window:Section("HSFL SECTION")
local sectionC = window:Section("OL/DL SECTION")
local sectionD = window:Section("TRACK SECTION")
local sectionF = window:Section("XCAA SECTION")
local sectionL = window:Section("LEGENDS ON CAMPUS")
local sectionX = window:Section("NHSFA SECTION")
window:Divider("UF")
local sectionG = window:Section("UF")
local sectionH = window:Section("KICKING SECTION")
local sectionI = window:Section("CONTROL SECTION")
local sectionJ = window:Section("SETTINGS")

-- RCFA
sectionA:Label("BYPASS")
sectionA:Button("MUST CLICK", function()
    game.Players.LocalPlayer.Character.Hitbox.LocalAntiCheat.Disabled = true
end)

sectionA:Label("MAGNET")
sectionA:TextField("RANGE", "Enter Magnet Range", function(b)
    game.Players.LocalPlayer.Character.Hitbox.Size = Vector3.new(b+3,b+5,b+3)
end)

sectionA:Label("HBE EXTENDER")
sectionA:TextField("RANGE", "Enter HBE Range", function(hbe)
    game.Players.LocalPlayer.Character.Hitbox.Size = Vector3.new(hbe,hbe+2,hbe)
end)

sectionA:Label("TUCK")
sectionA:Switch('VISIBLE HITBOX', false, function(vis)
    if vis then
        game.Players.LocalPlayer.Character.Hitbox.Transparency = 0.8
    else
        game.Players.LocalPlayer.Character.Hitbox.Transparency = 1
    end
end)

sectionA:Switch("AUTO TUCK", false, function(Tuc)
    local player = game.Players.LocalPlayer.Character
    player.ChildAdded:Connect(function(Tuc)
        if Tuc.Name == "Football" then
            wait(0.1)
            local args = {
                [1] = { [1] = "Tuck" }
            }
            game:GetService("Players").LocalPlayer.Character.Football.Handle.RemoteEvent:FireServer(unpack(args))
        end
    end)
end)

-- OL/DL ENHANCED BLOCKING
sectionC:Label("ENHANCED BLOCKING")
sectionC:Switch("ADVANCED", false, function(AdvancedBlocking)
    for _, player in pairs(game:GetService("Players"):GetPlayers()) do
        local character = player.Character
        if character and character.Name ~= game.Players.LocalPlayer.Name then
            local jersey = character:FindFirstChild("Jersey") or character:FindFirstChild("Hitbox")
            if jersey then
                jersey.Size = AdvancedBlocking and Vector3.new(10,10,10) or Vector3.new(2.31,5.4,2.3)
            end
        end
    end
end)

-- Legend On Campus
sectionL:Label("BYPASS")
sectionL:Button("MUST CLICK", function()
   game.Players.LocalPlayer.Character:FindFirstChild("Anticheat").Disabled = true
end)
sectionL:Label("MAGNET")
sectionL:Switch("VISIBLE HITBOX", false, function(reach)
    game.Players.LocalPlayer.Character.Jersey.Transparency = reach and 0.8 or 1
end)
sectionL:TextField("RANGE", "Enter Magnet Range", function(LOC)
    game.Players.LocalPlayer.Character.Jersey.Size = Vector3.new(LOC+3,LOC+5,LOC+3)
end)

-- HSFL
sectionB:Label("MAGNET")
sectionB:Switch("VISIBLE HITBOX", false, function(reach)
    game.Players.LocalPlayer.Character.Jersey.Transparency = reach and 0.8 or 1
end)
sectionB:TextField("RANGE", "Enter Magnet Range", function(hsfl3)
    game.Players.LocalPlayer.Character.Jersey.Size = Vector3.new(hsfl3+3,hsfl3+5,hsfl3+3)
end)
sectionB:Label("TUCK")
sectionB:Switch("Tuck On", false, function(autotuck)
    local player = game.Players.LocalPlayer.Character
    player.ChildAdded:Connect(function(TuckOn)
        if TuckOn.Name == "Football" then
            local args = { [1] = { [1] = "Tuck" } }
            game:GetService("Players").LocalPlayer.Character.Jersey.Event:FireServer(unpack(args))
        end
    end)
end)

-- GENERAL SECTION UF
sectionG:Label('GENERAL')
sectionG:TextField("Speed", "Enter Speed", function(Speed)
    local Humanoid = game.Players.LocalPlayer.Character.Humanoid
    Humanoid.WalkSpeed = Speed
end)

sectionG:TextField("Jump Power", "Enter Power", function(Jump)
    local Humanoid = game.Players.LocalPlayer.Character.Humanoid
    Humanoid.JumpPower = Jump
end)

sectionG:Label('PHYSICS')
sectionG:Switch("ANTI KNOCKBACK", false, function(antiknock)
    local a = game.Players.LocalPlayer.Character
    a["Left Arm"].Size = antiknock and Vector3.new(20,2,2) or Vector3.new(1,2,1)
    a["Right Arm"].Size = antiknock and Vector3.new(20,2,2) or Vector3.new(1,2,1)
end)

-- QB Follow
sectionG:Button("QB Follow (R)", function()
    local TOOL_NAME = "Football"
    local FOLLOW_DISTANCE = 1
    local character = game.Players.LocalPlayer.Character
    local isFollowing = false

    local function followPlayer()
        for _, player in pairs(game.Players:GetPlayers()) do
            local pc = player.Character
            if pc and pc:FindFirstChild(TOOL_NAME) then
                local torso = pc:FindFirstChild("HumanoidRootPart") or pc:FindFirstChild("Torso")
                if torso then
                    character:WaitForChild("Humanoid"):MoveTo(torso.Position + torso.CFrame.lookVector * -FOLLOW_DISTANCE, torso)
                end
            end
        end
    end

    game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
        if not gameProcessed and input.KeyCode == Enum.KeyCode.R then
            isFollowing = not isFollowing
        end
    end)

    game:GetService("RunService").RenderStepped:Connect(function()
        if isFollowing then followPlayer() end
    end)
end)

-- OTHER PLAYERS BIG HEAD FULL TRANSPARENT
sectionG:Label("OTHER PLAYERS BIG HEAD FULL TRANSPARENT")
local bigHeadEnabled = false
local headSize = 5
sectionG:Switch("BIG HEAD OTHERS", false, function(enabled)
    bigHeadEnabled = enabled
    for _, plr in pairs(game.Players:GetPlayers()) do
        if plr ~= game.Players.LocalPlayer and plr.Character and plr.Character:FindFirstChild("Head") then
            local head = plr.Character.Head
            head.Size = enabled and Vector3.new(headSize, headSize, headSize) or Vector3.new(2,2,2)
            head.Transparency = enabled and 1 or 0
        end
    end
end)
game.Players.PlayerAdded:Connect(function(plr)
    plr.CharacterAdded:Connect(function(char)
        task.wait(0.5)
        if bigHeadEnabled and char:FindFirstChild("Head") then
            char.Head.Size = Vector3.new(headSize, headSize, headSize)
            char.Head.Transparency = 1
        end
    end)
end)

-- BIG HANDS & ARMS â€” SLIGHTLY TRANSPARENT
sectionG:Label("BIG HANDS & ARMS")
local bigHandsArmsEnabled = false
local handSize = Vector3.new(4,4,4)
local armSize = Vector3.new(4,8,2)
local normalHandSize = Vector3.new(1,2,1)
local normalArmSize = Vector3.new(1,2,1)
local slightTransparency = 0.2

sectionG:Switch("BIG HANDS & ARMS", false, function(enabled)
    bigHandsArmsEnabled = enabled
    local char = game.Players.LocalPlayer.Character
    if not char then return end

    local leftArm = char:FindFirstChild("Left Arm") or char:FindFirstChild("LeftHand")
    local rightArm = char:FindFirstChild("Right Arm") or char:FindFirstChild("RightHand")

    if enabled then
        if leftArm then leftArm.Size = armSize leftArm.Transparency = slightTransparency end
        if rightArm then rightArm.Size = armSize rightArm.Transparency = slightTransparency end
    else
        if leftArm then leftArm.Size = normalArmSize leftArm.Transparency = 0 end
        if rightArm then rightArm.Size = normalArmSize rightArm.Transparency = 0 end
    end
end)

game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
    task.wait(0.5)
    if bigHandsArmsEnabled then
        local leftArm = char:FindFirstChild("Left Arm") or char:FindFirstChild("LeftHand")
        local rightArm = char:FindFirstChild("Right Arm") or char:FindFirstChild("RightHand")
        if leftArm then leftArm.Size = armSize leftArm.Transparency = slightTransparency end
        if rightArm then rightArm.Size = armSize rightArm.Transparency = slightTransparency end
    end
end)

-- IMMEDIATE SCRIPT EXECUTION
-- This will execute the script immediately when injected
local success, err = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/jamalyoung373739-bit/Mags/refs/heads/main/script"))()
end)
if not success then warn("Script Execution Failed:", err) end
